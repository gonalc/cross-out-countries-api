{
  "name": "yanapak-server",
  "version": "1.0.0",
  "description": "API for Yanapak platform",
  "main": "src/index.ts",
  "scripts": {
    "test": "jest --verbose  --detectOpenHandles --runInBand --forceExit",
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --pretty --transpile-only src/index.ts",
    "build": "rm -rf build/ && tsc",
    "start": "ts-node src/index.ts",
    "prestart": "npm run build",
    "prepare": "husky install",
    "lint": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,yml,json}": [
      "prettier --write"
    ]
  },
  "author": "Gonzalo Alcaide",
  "license": "ISC",
  "dependencies": {
    "@hapi/boom": "10.0.0",
    "axios": "1.4.0",
    "body-parser": "1.20.0",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dayjs": "1.11.7",
    "dotenv": "16.0.2",
    "express": "4.18.1",
    "helmet": "6.0.0",
    "http-status-codes": "2.2.0",
    "joi": "17.6.2",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "morgan": "1.10.0",
    "sequelize": "6.22.1",
    "winston": "3.8.2",
    "winston-daily-rotate-file": "4.7.1"
  },
  "devDependencies": {
    "@types/compression": "1.7.2",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.14",
    "@types/jest": "29.0.3",
    "@types/jsonwebtoken": "8.5.9",
    "@types/lodash": "4.14.185",
    "@types/morgan": "1.9.3",
    "@types/node": "18.7.18",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "cross-env": "7.0.3",
    "eslint": "8.23.1",
    "husky": "7.0.0",
    "jest": "29.0.3",
    "jest-cli": "29.0.3",
    "lint-staged": "13.0.3",
    "mysql2": "2.3.3",
    "nodemon": "2.0.20",
    "prettier": "2.7.1",
    "supertest": "6.2.4",
    "ts-jest": "29.0.1",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typescript": "4.8.3"
  }
}
